T := \x. \y. x
F := \x. \y. y

& := \p. \q. p q p
| := \p. \q. p p q
~ := \p. p F T

0 := \f. \x. x
S := \n. \f. \x. f (n f x)

1 := S 0
2 := S (S 0)
3 := S 2

+ := \m. \n. m S n
* := \m. \n. m (+ n) 0
^ := \b. \e. e b

P := \n. \f. \x. n
    (\g. \h. h (g f)) (\u. x) (\u. u)
- := \m. \n. n P m

Is0 := \n. n (\x. F) T
<= := \m. \n. Is0 (- m n)
== := \m. \n. & (<= m n) (<= n m)

Pair := \x. \y. \f. f x y
Fst := \p. p T
Snd := \p. p F

Apply2 := \f. \x. f (f x) -- Comment
Apply4 := Apply2
	Apply2

	Apply2

α := \β. β

A := B
B := A

C := C

-- A B
C

(|) F F
| F T
| T F
| T T

& F F
& F T
& T F
& T T

Is0 0
Is0 1
Is0 2
Is0 3
