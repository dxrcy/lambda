T := \x. \y. x
F := \x. \y. y

I := \x. x

& := \p. \q. p q p
| := \p. \q. p p q
~ := \p. p F T

0 := \f. \x. x
S := \n. \f. \x. f (n f x)

1 := S 0
2 := S (S 0)
3 := S 2
4 := S 3
5 := S 4
6 := S 5
7 := S 6

+ := \m. \n. \f. \x. m f (n f x)
+' := \m. \n. m S n

* := \m. \n. m (+ n) 0
^ := \b. \e. e b

P := \n. \f. \x. n
    (\g. \h. h (g f)) (\u. x) (\u. u)
- := \m. \n. n P m

Is0 := \n. n (\x. F) T
<= := \m. \n. Is0 (- m n)
== := \m. \n. & (<= m n) (<= n m)

Pair := \x. \y. \f. f x y
1st := \p. p T
2nd := \p. p F

Theta := \x. Pair (2nd x) (S (2nd x))

ApplyTwice := \f. \x. f (f x)

Omega := (\x. x x) (\x. x x)

